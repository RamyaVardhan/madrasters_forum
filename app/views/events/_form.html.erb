<%= simple_form_for @event do |f|%>
	<%= f.input :name %>
	<%= f.input :description %>
	<%= f.input :type_id %>
	<%= f.input :manager_id %>
	<%= f.input :event_type %>
	<%= f.input :count %>
	<%= f.input :venue %>
	<%= f.input :domain_id %>
	<%= f.input :location_id %>
	<%= f.input :speaker_id %>
	<%= f.input :team %>
	<%= f.input :agenda %>
	<%= f.input :event_date %>
	<%= f.input :helpline_number %>
	<%= f.submit %>
<%end%>